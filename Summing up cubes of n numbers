#include <stdio.h>

int main() {

  int n, i, num;

  int sum = 0;

  printf("Enter the number of integers to add: ");

  scanf("%d", &n);

  for (i = 1; i <= n; i++) {

    printf("Enter integer #%d: ", i);

    scanf("%d", &num);

    sum += num * num * num;

  }

  printf("The sum of the cubes is: %d\n", sum);

  return 0;

}

